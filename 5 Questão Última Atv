        printf("%d É divisivel apenas por 2, mas não por 3.\n", num);
    } else if (num % 3 == 0) {
        printf("%d é divisivel apenas por 3 mas não por 2.\n", num);
    } else {
        printf("%d Não é divisivel nem por 2 nem por 3.\n", num);
