#include <stdio.h>

int main()
{
  //Declaração de Variáveis
    float peso_ideal, altura; 
    float k = 72.7, b = 58;

  //Entrada de Dados
    printf ("Escreva a sua altura: ");
    scanf ("%f", &altura);
    
  //Saída de Dados
    printf ("O seu peso ideal é: %f", peso_ideal = (k * altura) - b);
    
}
