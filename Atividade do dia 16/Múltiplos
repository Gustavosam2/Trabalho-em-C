#include <stdio.h>
int main () {

//Declaração de variáveis
  int numero1, numero2;

//Entrada de Dados
  printf ("Informe o valor do primeiro número: ");
  scanf ("%d", &numero1);
  printf ("Informe o valor do segundo número: ");
  scanf ("%d", &numero2);

//Processamento
  if (numero1 % numero2==0){
    printf ("Os números são múltiplos ");
  } else{
    printf ("Os números não são múltiplos ");
  }


return 0;
}
