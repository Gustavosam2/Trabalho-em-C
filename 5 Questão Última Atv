#include <stdio.h>

int main() {
    int num;

    printf("Digite um numero inteiro: ");
    scanf("%d", &num);

    if ((num % 2 == 0) && (num % 3 == 0)) {
        printf("%d é divisivel por 2 e por 3 ao mesmo tempo.\n", num);
    } else if (num % 2 == 0) {
        printf("%d é divisivel apenas por 2, mas nao por 3.\n", num);
    } else if (num % 3 == 0) {
        printf("%d é divisivel apenas por 3 mas nao por 2.\n", num);
    } else {
        printf("%d nao é divisivel nem por 2 nem por 3.\n", num);
    }

    return 0;
}
